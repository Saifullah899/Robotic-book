{
  "version": 2,
  "framework": "docusaurus",
  "settings": {
    "buildCommand": "cd website && npm install && npm run build",
    "outputDirectory": "website/build",
    "rootDirectory": "website"
  },
  "builds": [
    {
      "src": "website/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "website/index.html"
    }
  ]
}